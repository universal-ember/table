{
  "name": "docs-app",
  "version": "0.0.0",
  "type": "module",
  "exports": {
    "./tests/*": "./tests/*",
    "./*": "./src/*"
  },
  "imports": {
    "#src/*": "./src/*",
    "#data": "./src/sample-data/index.ts",
    "#config": "./src/config.ts",
    "#components/*": "./src/components/*"
  },
  "private": true,
  "description": "Documentation for the @universal-ember/table addon",
  "repository": "https://github.com/universal-ember/table",
  "license": "MIT",
  "author": "NullVoxPopuli",
  "scripts": {
    "build:prod": "vite build && cp -r ../docs-api/dist ./dist/api",
    "build:test": "NODE_ENV=development vite build --mode test",
    "format": "prettier . --cache --write",
    "lint": "concurrently \"npm:lint:*(!fix)\" --names \"lint:\" --prefixColors auto",
    "lint:fix": "concurrently \"npm:lint:*:fix\" --names \"fix:\" --prefixColors auto && npm run format",
    "lint:format": "prettier . --cache --check",
    "lint:hbs": "ember-template-lint .",
    "lint:hbs:fix": "ember-template-lint . --fix",
    "lint:js": "eslint . --cache",
    "lint:js:fix": "eslint . --fix",
    "lint:types": "glint",
    "start": "vite",
    "test": "pnpm test:ember",
    "test:ember": "pnpm build:test && pnpm dist:ember:test",
    "dist:ember:test": "ember test --path dist --config-file ./testem.cjs"
  },
  "devDependencies": {
    "@babel/core": "^7.29.0",
    "@ember/optional-features": "^3.0.0",
    "@ember/test-helpers": "^5.4.1",
    "@embroider/compat": "^4.1.13",
    "@embroider/core": "^4.4.3",
    "@embroider/macros": "^1.19.7",
    "@embroider/vite": "^1.5.2",
    "@glimmer/component": "^2.0.0",
    "@glint/core": "1.5.2",
    "@glint/environment-ember-loose": "1.5.2",
    "@glint/environment-ember-template-imports": "1.5.2",
    "@glint/template": "1.7.4",
    "@html-next/vertical-collection": "^5.0.2",
    "@rollup/plugin-babel": "^6.1.0",
    "@tailwindcss/typography": "^0.5.19",
    "@tsconfig/ember": "^3.0.12",
    "@types/dompurify": "^3.2.0",
    "@types/qunit": "^2.19.13",
    "@types/rsvp": "^4.0.9",
    "autoprefixer": "^10.4.24",
    "concurrently": "^9.2.1",
    "cssnano": "^7.1.2",
    "decorator-transforms": "^2.3.1",
    "ember-auto-import": "^2.12.0",
    "ember-cli": "~6.11.0",
    "ember-eslint": "^0.6.1",
    "ember-page-title": "^9.0.3",
    "ember-qunit": "^9.0.4",
    "ember-resize-observer-service": "^1.1.0",
    "ember-resolver": "^13.2.0",
    "ember-source": "~6.11.0",
    "ember-template-lint": "^7.9.3",
    "eslint": "^9.22.0",
    "postcss": "^8.5.6",
    "postcss-import": "^16.1.1",
    "prettier": "^3.8.1",
    "prettier-plugin-ember-template-tag": "^2.1.3",
    "qunit": "^2.25.0",
    "qunit-dom": "^3.5.0",
    "tailwindcss": "^3.4.17",
    "typescript": "~5.9.3",
    "vite": "7.3.1"
  },
  "ember": {
    "edition": "octane"
  },
  "ember-addon": {
    "type": "app",
    "version": 2
  },
  "dependencies": {
    "@ember/test-waiters": "^4.1.1",
    "@embroider/router": "3.0.6",
    "@shikijs/rehype": "^3.22.0",
    "@universal-ember/docs-support": "^0.9.2",
    "@universal-ember/table": "workspace:*",
    "content-tag": "^4.1.0",
    "dompurify": "^3.3.1",
    "ember-modifier": "^4.3.0",
    "ember-primitives": "^0.53.0",
    "ember-repl": "^8.0.2",
    "ember-resources": "^7.0.7",
    "kolay": "^5.1.3",
    "reactiveweb": "^1.9.1",
    "shiki": "^3.22.0",
    "tracked-built-ins": "^4.1.0"
  },
  "dependenciesMeta": {
    "@universal-ember/table": {
      "injected": true
    }
  },
  "volta": {
    "extends": "../package.json"
  }
}
